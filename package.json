    "@sentry/integrations": "^5.8.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "ajv": "^6.6.2",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.0.4",
    "chart.js": "^2.7.3",
    "vue": "^2.6.11",
    "vue-analytics": "^5.16.1",
    "vue-chartjs": "^3.4.0",
    "vue-json-csv": "^1.2.11",
